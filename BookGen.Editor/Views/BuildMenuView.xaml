<UserControl
    x:Class="BookGen.Editor.Views.BuildMenuView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="50"
    mc:Ignorable="d">
    <ToolBarTray Orientation="Vertical">
        <ToolBar>
            <Button
                Content="{StaticResource BuildTest}"
                ToolTip="Build Test website..." />
            <Button
                Content="{StaticResource BuildWeb}"
                ToolTip="Build Website..." />
            <Button
                Content="{StaticResource BuildPrint}"
                ToolTip="Build printable HTML..." />
            <Button
                Content="{StaticResource BuildBook}"
                ToolTip="Build Epub..." />
            <Button
                Content="{StaticResource BuildWordpress}"
                ToolTip="Build Wordpress compatible export..." />
        </ToolBar>
    </ToolBarTray>
</UserControl>
